{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-spacing": "off",
        "max-len": ["error", { "code": 120 }],
        "new-cap": "off",
        "require-jsdoc": "off"
    }
}
